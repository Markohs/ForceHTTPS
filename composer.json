{
    "name": "markohs/forcehttps",
    "description": "Simple Laravel Middleware to force HTTPS connections",
    "license": "MIT",
    "authors": [
        {
            "name": "Markohs",
            "email": "marcos@tyrellcorporation.es",
            "homepage": "https://github.com/Markohs"
        }
    ],
    "homepage": "https://github.com/markohs/forcehttps",
    "keywords": ["Laravel", "ForceHTTPS"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Markohs\\ForceHTTPS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Markohs\\ForceHTTPS\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Markohs\\ForceHTTPS\\ForceHTTPSServiceProvider"
            ],
            "aliases": {
                "ForceHTTPS": "Markohs\\ForceHTTPS\\Facades\\ForceHTTPS"
            }
        }
    }
}
